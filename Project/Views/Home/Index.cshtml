@model HomeViewModel
<div class="container">
    <div class="container mt-2 mb-3" style="">
        <partial name="_Carousel"/>
    </div>
    

    <h2>Courses of the week</h2>
    <h4>Our weekly selection - just for you!</h4>

    <div class="row">
        @foreach(var course in Model.CoursesOfTheWeek)
        {
            <partial name="_CourseCard" model="course" />
        }
    </div>
</div>
